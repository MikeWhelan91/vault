{
  "name": "vault-worker-api",
  "version": "1.0.0",
  "description": "Cloudflare Worker API for Zero-Knowledge Vault R2 storage",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "r2:create": "wrangler r2 bucket create vault-data",
    "r2:create:dev": "wrangler r2 bucket create vault-data-dev",
    "r2:list": "wrangler r2 bucket list",
    "r2:info": "wrangler r2 bucket info vault-data"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "r2",
    "encryption",
    "zero-knowledge"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "wrangler": "^3.94.0",
    "typescript": "^5.7.2"
  }
}
